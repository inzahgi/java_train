<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.fwpt.mapper.taskShow.ZDYResultMapper">
	<resultMap id="ZDYResultVo" type="com.iflytek.fwpt.model.taskShow.ZDYResultVo">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="rule_id" property="ruleId" jdbcType="VARCHAR" />
		<result column="task_type" property="taskType" jdbcType="VARCHAR" />
		<result column="acc_type" property="accType" jdbcType="VARCHAR" />
		<result column="account" property="account" jdbcType="VARCHAR" />
		<result column="warning_time" property="warningTime" jdbcType="TIMESTAMP" />
		<result column="destfrontendname" property="destfrontendname" jdbcType="VARCHAR" />
		<result column="tbname" property="tbName" jdbcType="VARCHAR" />
		<result column="category" property="category" jdbcType="VARCHAR" />
		<result column="columns_json" property="columnsJSON" jdbcType="VARCHAR" />
		<result column="idReaded" property="idReaded" jdbcType="VARCHAR" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="save">
		INSERT INTO tbl_zdy_result (
			id,
			rule_id,
			task_type,
			acc_type,
			account,
			warning_time,
			destfrontendname,
			tbname,
			category,
			columns_json,
			idReaded,
			createtime
		)
		VALUES
			(
				#{id},
				#{ruleId},
				#{taskType},
				#{accType},
				#{account},
				#{warningTime},
				#{destfrontendname},
				#{tbName},
				#{category},
				#{columnsJSON},
				#{idReaded},
				now()
			)
	</insert>

</mapper>