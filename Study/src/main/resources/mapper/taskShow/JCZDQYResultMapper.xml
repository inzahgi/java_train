<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.fwpt.mapper.taskShow.JCZDQYResultMapper">
	<resultMap id="JCZDQYResultVo" type="com.iflytek.fwpt.model.taskShow.JCZDQYResultVo">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="rule_id" property="ruleId" jdbcType="VARCHAR" />
		<result column="task_type" property="taskType" jdbcType="VARCHAR" />
		<result column="account" property="account" jdbcType="VARCHAR" />
		<result column="status" property="status" jdbcType="VARCHAR" />
		<result column="staytime" property="staytime" jdbcType="VARCHAR" />
		<result column="warning_time" property="warningTime" jdbcType="TIMESTAMP" />
		<result column="area_id" property="areaId" jdbcType="VARCHAR" />
		<result column="old_location" property="oldLocation" jdbcType="VARCHAR" />
		<result column="idReaded" property="idReaded" jdbcType="VARCHAR" />
		<result column="account_json" property="accountJSON" jdbcType="VARCHAR" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="save">
		INSERT INTO tbl_jczdqy_result (
			id,
			rule_id,
			task_type,
			account,
			status,
			staytime,
			capturetime,
			area_id,
			old_location,
			idReaded,
			account_json,
			createtime
		)
		VALUES
			(
				#{id},
				#{ruleId},
				#{taskType},
				#{account},
				#{status},
				#{staytime},
				#{warningTime},
				#{areaId},
				#{oldLocation},
				#{idReaded},
				#{accountJSON},
				now()
			)
	</insert>

</mapper>