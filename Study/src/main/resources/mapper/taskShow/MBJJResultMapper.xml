<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.fwpt.mapper.taskShow.MBJJResultMapper">
	<resultMap id="MBJJResultVo" type="com.iflytek.fwpt.model.taskShow.MBJJResultVo">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="rule_id" property="ruleId" jdbcType="VARCHAR" />
		<result column="task_type" property="taskType" jdbcType="VARCHAR" />
		<result column="cent_lon" property="centLon" jdbcType="VARCHAR" />
		<result column="cent_lat" property="centLat" jdbcType="VARCHAR" />
		<result column="gatherradius" property="gatherradius" jdbcType="VARCHAR" />
		<result column="warning_time" property="warningTime" jdbcType="VARCHAR" />
		<result column="idReaded" property="idReaded" jdbcType="VARCHAR" />
		<result column="account_json" property="accountJSON" jdbcType="VARCHAR" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="save">
		INSERT INTO tbl_mbjj_result (
			id,
			rule_id,
			task_type,
			cent_lon,
			cent_lat,
			gatherradius,
			warning_time,
			idReaded,
			account_json,
			createtime
		)
		VALUES
			(
				#{id},
				#{ruleId},
				#{taskType},
				#{cent_lon},
				#{cent_lat},
				#{gatherradius},
				#{warningTime},
				#{idReaded},
				#{accountJSON},
				now()
			)
	</insert>

</mapper>