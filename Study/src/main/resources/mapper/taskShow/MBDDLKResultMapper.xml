<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.fwpt.mapper.taskShow.MBDDLKResultMapper">
	<resultMap id="MBDDLKResultVo" type="com.iflytek.fwpt.model.taskShow.MBDDLKResultVo">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="rule_id" property="ruleId" jdbcType="VARCHAR" />
		<result column="task_type" property="taskType" jdbcType="VARCHAR" />
		<result column="acc_type" property="accType" jdbcType="VARCHAR" />
		<result column="account" property="account" jdbcType="VARCHAR" />
		<result column="action" property="action" jdbcType="VARCHAR" />
		<result column="area_type" property="areaType" jdbcType="VARCHAR" />
		<result column="area_code" property="areaCode" jdbcType="VARCHAR" />
		<result column="last_time" property="lastTime" jdbcType="TIMESTAMP" />
		<result column="old_last_time" property="oldLastTime" jdbcType="TIMESTAMP" />
		<result column="last_location" property="lastLocation" jdbcType="VARCHAR" />
		<result column="leave_type" property="leaveType" jdbcType="VARCHAR" />
		<result column="old_cgi" property="oldCgi" jdbcType="VARCHAR" />
		<result column="old_net" property="oldNet" jdbcType="VARCHAR" />
		<result column="old_lon" property="oldLon" jdbcType="VARCHAR" />
		<result column="old_lat" property="oldLat" jdbcType="VARCHAR" />
		<result column="old_cur_area" property="oldCurArea" jdbcType="VARCHAR" />
		<result column="old_area_code" property="oldAreaCode" jdbcType="VARCHAR" />
		<result column="last_area" property="lastArea" jdbcType="VARCHAR" />
		<result column="idReaded" property="idReaded" jdbcType="VARCHAR" />
		<result column="account_json" property="accountJSON" jdbcType="VARCHAR" />
		<result column="warning_time" property="warningTime" jdbcType="TIMESTAMP" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="save">
		INSERT INTO tbl_mbddlk_result (
			id,
			rule_id,
			task_type,
			acc_type,
			account,
			action,
			area_type,
			area_code,
			last_time,
			last_location,
			leave_type,
			old_cgi,
			old_net,
			old_lon,
			old_lat,
			old_cur_area,
			old_area_code,
			old_last_time,
			last_area,
			idReaded,
			account_json,
			warning_time,
			createtime
		)
		VALUES
			(
				#{id},
				#{ruleId},
				#{taskType},
				#{accType},
				#{account},
				#{action},
				#{areaType},
				#{areaCode},
				#{lastTime},
				#{lastLocation},
				#{leaveType},
				#{oldCgi},
				#{oldNet},
				#{oldLon},
				#{oldLat},
				#{oldCurArea},
				#{oldAreaCode},
				#{oldLastTime},
				#{lastArea},
				#{idReaded},
				#{accountJSON},
				#{warningTime},
				now()
			)
	</insert>

</mapper>