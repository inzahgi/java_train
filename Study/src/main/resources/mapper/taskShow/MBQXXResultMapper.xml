<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.fwpt.mapper.taskShow.MBQXXResultMapper">
	<resultMap id="MBQXXResultVo" type="com.iflytek.fwpt.model.taskShow.MBQXXResultVo">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="rule_id" property="ruleId" jdbcType="VARCHAR" />
		<result column="task_type" property="taskType" jdbcType="VARCHAR" />
		<result column="membernum" property="membernum" jdbcType="VARCHAR" />
		<result column="idReaded" property="idReaded" jdbcType="VARCHAR" />
		<result column="account_json" property="accountJSON" jdbcType="VARCHAR" />
		<result column="warning_time" property="warningTime" jdbcType="TIMESTAMP" />
		<result column="createtime" property="createtime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="save">
		INSERT INTO tbl_mbqxx_result (
			id,
			rule_id,
			task_type,
			membernum,
			idReaded,
			account_json,
			warning_time,
			createtime
		)
		VALUES
			(
				#{id},
				#{ruleId},
				#{taskType},
				#{membernum},
				#{idReaded},
				#{accountJSON},
				#{warningTime},
				now()
			)
	</insert>

</mapper>